{
  "name": "seriousgame",
  "displayName": "Orange XDLAB - Serious Game Builder",
  "version": "1.0.0",
  "description": "Cr√©ateur de jeu Orange",
  "author": "Yohann CAILLON",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.3.1",
    "@material-ui/icons": "^3.0.1",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "halogenium": "^2.2.3",
    "http": "0.0.0",
    "i18n-react": "^0.6.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "3.1.0",
    "redux": "^4.0.0",
    "socket.io": "^2.1.1",
    "validator": "^10.8.0"
  },
  "main": "./build/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && copy main.js build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "electron-dev": "copy main.js build && set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "electron": "electron .",
    "dist": "build"
  },
  "build": {
    "files": "build",
    "appId": "yourappid",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "public/favicon.ico"
    }
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.3",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-redux": "^6.0.9",
    "@types/redux": "^3.6.0",
    "@types/socket.io-client": "^1.4.32",
    "@types/validator": "^9.4.2",
    "electron": "^3.0.8",
    "electron-builder": "^20.34.0",
    "typescript": "^3.1.1"
  },
  "files": [
    "*.js",
    "build",
    "node_modules"
  ]
}
